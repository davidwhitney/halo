{
  "name": "halofx",
  "version": "0.0.3",
  "description": "A better TypeScript webapp framework",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "vitest",
    "build": "tsup src/index.ts",
    "example:minimalapi": "tsx watch ./examples/minimal-api-like/index.tsx",
    "deno:example:minimalapi": "deno run --allow-net --allow-env --unstable-sloppy-imports --watch ./examples/minimal-api-like/index.tsx",
    "deno:example:mvclike": "deno run --allow-net --allow-env --unstable-sloppy-imports --watch ./examples/mvc-like/index.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/davidwhitney/halo.git"
  },
  "keywords": [
    "halo",
    "http",
    "rest",
    "api",
    "app",
    "server"
  ],
  "author": "David Whitney",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidwhitney/halo/issues"
  },
  "homepage": "https://github.com/davidwhitney/halo#readme",
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5"
  },
  "dependencies": {
    "fast-xml-parser": "^4.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
